@model IEnumerable<RestaurantNew.Models.Order>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Menus.NameDose)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdClub)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateForDay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Count)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Menus.NameDose)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdClub)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateForDay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Count)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdOrder }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdOrder }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdOrder })
        </td>
    </tr>
}

</table>
